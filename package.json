{
  "name": "nyaapi-http-client",
  "version": "0.0.1",
  "description": "nyaa.si http client [unofficial]",
  "main": "dist/index.js",
  "author": "Raul Max <raulmax319@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "build": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "build:paths": "npx tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:clean": "npm run clean && yarn build && yarn build:paths",
    "clean": "rm -rf dist",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src",
    "lint": "eslint src/**",
    "lint:staged": "npx lint-staged"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^18.0.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
